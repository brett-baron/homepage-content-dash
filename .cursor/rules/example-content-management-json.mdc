            "fields": {
                "internalName": {
                    "en-US": "Introducing the Coin mobile app"
                },
                "postName": {
                    "de-DE": "Einführung in die Coin Mobile-App",
                    "en-US": "Introducing the Coin mobile app"
                },
                "slug": {
